{
  "main": "./dist/vue-capture-media.common.js",
  "name": "@sanjac23/vue-capture-media",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build-bundle": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build --target lib --name vue-capture-media ./src/components/index.js",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "lint": "export NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service lint",
    "dev": "code . & npm run serve",
    "commit": "sh scripts/commit.sh"
  },
  "dependencies": {
    "vue": "^2.6.14",
    "vue-router": "^3.4.9",
    "vue-slider-component": "^3.2.24",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.51.0",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "author": {
    "name": "Sanja Cornelius",
    "email": "sanjacornelius@gmail.com"
  },
  "repository": "https://github.com/sanjacornelius/vue-capture-media",
  "keywords": [
    "vue",
    "vueJS",
    "html5",
    "js",
    "web-sockets",
    "media-devices",
    "media-recorder"
  ],
  "license": "MIT",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ]
}
